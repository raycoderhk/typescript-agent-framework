{
  "name": "nullshot-cli",
  "version": "1.0.0",
  "author": "Felix NordÃ©n",
  "description": "CLI tool for managing Model Context Protocol (MCP) servers with Cloudflare Workers and Durable Objects",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "cloudflare-workers",
    "durable-objects",
    "cli",
    "typescript"
  ],
  "files": [
    "dist",
    "schema.json"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nullshot/nullshot-cli.git"
  },
  "homepage": "https://github.com/nullshot/nullshot-cli#readme",

{
  "name": "@null-shot/test-utils",
  "version": "0.3.0",
  "description": "Testing utilities for Cloudflare Workers projects - designed specifically for Workers runtime",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "worker": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./mcp/WorkerSSEClientTransport": {
      "types": "./dist/mcp/WorkerSSEClientTransport.d.ts",
      "import": "./dist/mcp/WorkerSSEClientTransport.js",
      "default": "./dist/mcp/WorkerSSEClientTransport.js"
    },
    "./mcp/WorkerWebSocketClientTransport": {
      "types": "./dist/mcp/WorkerWebSocketClientTransport.d.ts",
      "import": "./dist/mcp/WorkerWebSocketClientTransport.js",
      "default": "./dist/mcp/WorkerWebSocketClientTransport.js"
    },
    "./src": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./vitest/mcpWorkersConfig": {
      "types": "./dist/vitest/mcpWorkersConfig.d.ts",
      "import": "./dist/vitest/mcpWorkersConfig.js",
      "default": "./dist/vitest/mcpWorkersConfig.js"
    },
    "./vitest/ajv-mock": {
      "types": "./dist/vitest/ajv-mock.d.ts",
      "import": "./dist/vitest/ajv-mock.js",
      "default": "./dist/vitest/ajv-mock.js"
    },
    "./dist/vitest/mcpWorkersConfig.js": {
      "types": "./dist/vitest/mcpWorkersConfig.d.ts",
      "import": "./dist/vitest/mcpWorkersConfig.js",
      "default": "./dist/vitest/mcpWorkersConfig.js"
    }
  },
  "engines": {
    "workerd": ">=1.0.0"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "testing",
    "mcp",
    "vitest"
  ],
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "test": "echo Success",
    "lint": "echo 'No ESLint configured for this package'",
    "lint:fix": "echo 'No ESLint configured for this package'",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm build",
    "postversion": "echo 'Version updated to' $npm_package_version"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "catalog:",
    "@cloudflare/workers-types": "catalog:",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^24.2.3",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
